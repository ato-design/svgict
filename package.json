{
  "name": "svgict",
  "version": "0.0.2",
  "description": "Simple svg icon creator",
  "keywords": [
    "svg icon"
  ],
  "main": "dist/svgict.js",
  "unpkg": "dist/svgict.min.js",
  "module": "dist/svgict.js",
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "svgict": "./bin/svgict"
  },
  "files": [
    "bin",
    "src"
  ],
  "author": "Anowlet",
  "repository": {
    "type": "git",
    "url": "https://github.com/ato-design/svgict.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">12.0.0"
  },
  "scripts": {
    "svgict": "node ./bin/svgict",
    "build": "./bin/build.sh",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "prebuild": "rimraf lib dist es coverage types",
    "build-react": "rm -rf dist/icons && node bin/build-react.js",
    "build-react:bundle": "rm -rf build && rollup --config rollup.config.js",
    "build-react:es": "rm -rf dist && babel src --out-dir dist --copy-files",
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "release": "semantic-release",
    "semantic-release-prepare": "ts-node scripts/semantic-release-prepare",
    "precommit": "lint-staged",
    "prepush": "npm run test:prod && npm run build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.1",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "lint-staged": "^10.2.8",
    "npm-run-all": "^4.1.2",
    "prompt": "^1.0.0",
    "replace-in-file": "^6.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.8",
    "webpack": "^4.8.3"
  },
  "dependencies": {
    "@vue/compiler-dom": "^3.0.5",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.2",
    "coa": "^2.0.2",
    "fs-extra": "^9.0.1",
    "html-minifier": "^3.5.8",
    "prettier": "^2.0.5",
    "svgo": "^0.7.2",
    "uppercamelcase": "^3.0.0"
  }
}
